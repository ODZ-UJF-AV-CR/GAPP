{
    "name": "@gapp/shared",
    "version": "0.0.1",
    "type": "module",
    "private": true,
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "pnpm clean && tsup src/index.ts --format esm --dts --clean --minify",
        "dev": "tsup src/index.ts --format esm --watch --dts",
        "clean": "rm -rf dist"
    },
    "devDependencies": {
        "@types/node": "^25.3.0",
        "tsup": "^8.5.1",
        "typescript": "~5.9.3"
    },
    "dependencies": {
        "@fastify/type-provider-typebox": "^6.1.0"
    }
}
